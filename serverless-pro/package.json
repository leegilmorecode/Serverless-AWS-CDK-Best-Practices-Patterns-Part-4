{
  "name": "serverless-pro",
  "version": "0.1.0",
  "bin": {
    "serverless-pro": "bin/pipeline.js"
  },
  "scripts": {
    "synth": "cdk synth --all",
    "format": "npx prettier --write .",
    "lint": "eslint --ext .ts --ext .tsx .",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx .",
    "prepare": "cd .. && husky install config/.husky",
    "precommit": "npm run synth && npm run lint:fix && npm run format",
    "prepush": "npm run test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "jest:clear": "jest --clearCache",
    "deploy:pipeline": "cdk deploy ServerlessPro --profile=featuredev",
    "remove:pipeline": "cdk destroy ServerlessPro --profile=featuredev",
    "predeploy:dev": "cd ../client/ && npm run build && cd ../serverless-pro/",
    "deploy:dev": "cdk deploy ServerlessPro/Develop-PR-888/StatefulStack ServerlessPro/Develop-PR-888/StatelessStack ServerlessPro/Develop-PR-888/ClientStack ServerlessPro/Develop-PR-888/FeatureFlagStack --profile=featuredev --require-approval=never",
    "preremove:dev": "cdk synth --profile=featuredev",
    "remove:dev": "cdk destroy ServerlessPro/Develop-PR-888/StatefulStack ServerlessPro/Develop-PR-888/StatelessStack ServerlessPro/Develop-PR-888/ClientStack ServerlessPro/Develop-PR-888/FeatureFlagStack --profile=featuredev --require-approval=never"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/convict": "^6.1.1",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "ajv": "^8.12.0",
    "artillery": "^2.0.0-30",
    "artillery-plugin-ensure": "^1.1.1",
    "artillery-plugin-expect": "^2.2.1",
    "aws-cdk": "^2.70.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "@aws-cdk/aws-synthetics-alpha": "^2.70.0-alpha.0",
    "@aws-lambda-powertools/logger": "^1.8.0",
    "@aws-lambda-powertools/metrics": "^1.8.0",
    "@aws-lambda-powertools/tracer": "^1.8.0",
    "@middy/core": "^4.3.1",
    "aws-cdk-lib": "^2.70.0",
    "aws-sdk": "^2.1304.0",
    "axios": "^1.3.6",
    "cdk-nag": "^2.22.10",
    "constructs": "^10.0.0",
    "convict": "^6.2.4",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
